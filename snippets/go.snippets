snippet ilog
	"mabetle/mcore/mlog"
# test func
snippet fnt
	func Test${1:XXX}(t *testing.T){
		mtest.RegTest(t)
		mtest.Assert${2:Equal}(${3:true},${4:true})
	}

	${0}
# main()
snippet ma
	func main() {
		${1}
	}
	${0}

# package main
snippet pm
	package main

	${0}
